{
  "name": "WeedAR",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "start:reset": "npx react-native start --reset-cache",
    "android:run": "ENVFILE=.env.staging npx react-native run-android --variant=stagingDebug --appIdSuffix=staging",
    "android:run:production": "ENVFILE=.env.production npx react-native run-android --variant=productionDebug",
    "ios:run": "npx react-native run-ios --scheme \"Staging\"",
    "ios:run:production": "npx react-native run-ios --scheme \"Production\"",
    "lint": "eslint 'app/**/*.{js,ts,tsx}' --fix",
    "lint:fix": "yarn lint --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && yarn cache clean && yarn install",
    "clean:android": "cd ./android && ./gradlew clean && ./gradlew cleanBuildCache",
    "assets:link": "npx react-native link",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:avd": "cd $ANDROID_HOME/tools/bin && ./avdmanager list avd",
    "linking:deep": "npx uri-scheme open url",
    "linking:deep:production": "npx uri-scheme open url",
    "ios:devices": "xcrun simctl list devices",
    "pods:reinstall": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "4.4.5",
    "@gorhom/portal": "1.0.14",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "6.5.0",
    "@react-navigation/drawer": "6.5.4",
    "@react-navigation/native": "6.1.0",
    "@react-navigation/stack": "6.3.8",
    "@reduxjs/toolkit": "1.9.1",
    "@styled-system/props": "5.1.5",
    "@styled-system/theme-get": "5.1.2",
    "axios": "1.2.1",
    "final-form": "4.20.7",
    "final-form-arrays": "3.0.2",
    "i18n-js": "4.2.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-native": "0.70.6",
    "react-native-config": "1.4.11",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-localize": "2.2.4",
    "react-native-modal": "13.0.1",
    "react-native-reanimated": "2.13.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "13.6.0",
    "react-native-version-number": "0.3.6",
    "react-native-webview": "11.26.0",
    "react-redux": "8.0.5",
    "redux-flipper": "2.0.2",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.2",
    "styled-components": "5.3.6",
    "styled-system": "5.1.5",
    "uuid": "9.0.0",
